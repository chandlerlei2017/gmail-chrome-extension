{
    "manifest_version": 2,
    "name": "GDE Sample: Chrome extension Google APIs",
    "version": "2",
    "icons": {
        "16": "img/developers-logo.png",
        "48": "img/developers-logo.png",
        "128": "img/developers-logo.png"
    },
    "permissions": [
        "background",
        "identity",
        "notifications",
        "storage",
        "alarms",
        "https://www.googleapis.com/*",
        "https://*.googleusercontent.com/*"
    ],
    "background": {
        "scripts": [
            "js/background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_icon": {
            "19": "img/gmail-logo.png",
            "38": "img/gmail-logo.png"
        },
        "default_title": "Click to Sign in with Google"
    },
    "oauth2": {
        "client_id": "68543515779-fs3cna45k6bs57t094scd7vm80oa3lif.apps.googleusercontent.com",
        "scopes": [
            "profile",
            "https://www.googleapis.com/auth/gmail.readonly"
        ]
    },
    "key": ""
}
